{
  "manifest_version": 3,
  "name": "AI Portal",
  "version": "1.0",
  "description": "Access AI services directly from the Chrome address bar",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/icon16.svg",
    "48": "icons/icon48.svg",
    "128": "icons/icon128.svg"
  }
}
